public class Repair_Car {
	private String rep_id;//还车编号
	private String rep_car_id;//车牌号
	private String rep_maintain;//修理厂
	private String rep_details;//维修详情
	private String rep_out_time;//开始时间
	private String rep_return_time;//结束时间
	private String rep_price;//维修价格
	private String rep_inputer;//记录人
	private String rep_notes;//维修备注
	
	/**
	 * get and set
	 */
	public String getRep_id() {
		return rep_id;
	}
	public void setRep_id(String rep_id) {
		this.rep_id = rep_id;
	}
	public String getRep_car_id() {
		return rep_car_id;
	}
	public void setRep_car_id(String rep_car_id) {
		this.rep_car_id = rep_car_id;
	}
	public String getRep_maintain() {
		return rep_maintain;
	}
	public void setRep_maintain(String rep_maintain) {
		this.rep_maintain = rep_maintain;
	}
	public String getRep_details() {
		return rep_details;
	}
	public void setRep_details(String rep_details) {
		this.rep_details = rep_details;
	}
	public String getRep_out_time() {
		return rep_out_time;
	}
	public void setRep_out_time(String rep_out_time) {
		this.rep_out_time = rep_out_time;
	}
	public String getRep_return_time() {
		return rep_return_time;
	}
	public void setRep_return_time(String rep_return_time) {
		this.rep_return_time = rep_return_time;
	}
	public String getRep_price() {
		return rep_price;
	}
	public void setRep_price(String rep_price) {
		this.rep_price = rep_price;
	}
	public String getRep_inputer() {
		return rep_inputer;
	}
	public void setRep_inputer(String rep_inputer) {
		this.rep_inputer = rep_inputer;
	}
	public String getRep_notes() {
		return rep_notes;
	}
	public void setRep_notes(String rep_notes) {
		this.rep_notes = rep_notes;
	}
	public Repair_Car(String rep_id, String rep_car_id, String rep_maintain, String rep_details, String rep_out_time,
			String rep_return_time, String rep_price, String rep_inputer, String rep_notes) {
		super();
		this.rep_id = rep_id;
		this.rep_car_id = rep_car_id;
		this.rep_maintain = rep_maintain;
		this.rep_details = rep_details;
		this.rep_out_time = rep_out_time;
		this.rep_return_time = rep_return_time;
		this.rep_price = rep_price;
		this.rep_inputer = rep_inputer;
		this.rep_notes = rep_notes;
	}
	public Repair_Car() {
		super();
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return "Repair_Car [rep_id=" + rep_id + ", rep_car_id=" + rep_car_id + ", rep_maintain=" + rep_maintain
				+ ", rep_details=" + rep_details + ", rep_out_time=" + rep_out_time + ", rep_return_time="
				+ rep_return_time + ", rep_price=" + rep_price + ", rep_inputer=" + rep_inputer + ", rep_notes="
				+ rep_notes + "]";
	}
	
	
}
